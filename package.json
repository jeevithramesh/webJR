{
  "name": "web-jr",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run shell\" \"npm run server\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "shell": "node --max_old_space_size=3000 ./node_modules/@angular/cli/bin/ng serve webJR --proxy-config proxy/proxy.json -o",
    "mfe1": "node --max_old_space_size=2000 ./node_modules/@angular/cli/bin/ng serve mfe1",
    "mfe2": "node --max_old_space_size=2000 ./node_modules/@angular/cli/bin/ng serve mfe2",
    "server": "nodemon ./index.js --watch ./index.js"
  },
  "private": true,
